fun main() {
    var tep = 5
    var tep1 = 7
    var tep2 = "like"// 변수 선언 형식
    
    tep=20// 값의 리셋
    
    // tep1="love" 
    // tep1=2.0
    // 처럼 값의 자료형을 변화시킬 수는 없음
    
    println("코틀린은 정말 좋군요!!!")
    println(tep)
    println(tep + tep1)
    println("문자열" + " " + tep2)
    //println(tep + tep2) -> 오류
    println(tep2 + tep1)
    //출력과 합성
    
}
